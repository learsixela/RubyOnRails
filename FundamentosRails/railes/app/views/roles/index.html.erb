<p id="notice"><%= notice %></p>

<h1>Rols</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roles.each do |rol| %>
      <tr>
        <td><%= rol.nombre %></td>
        <td><%= link_to 'Show', rol %></td>
        <td><%= link_to 'Edit', edit_role_path(rol) %></td>
        <td><%= link_to 'Destroy', rol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rol', new_rol_path %>


<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% @us.each do |usuario| %>
    <tr><td><%= usuario.nombre %></td></tr>   
  <% end %>
  </tbody>
</table>